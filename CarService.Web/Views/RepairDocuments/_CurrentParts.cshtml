@model RepairDocumentBase

<div class="text-center"><label>Current replacement parts</label></div>
<div>
    <table id="showUsedParts" class="table table-condensed table-responsive table-bordered">
        <tr>
            <th width="20%">Catalog number</th>
            <th width="50%">Name</th>
            <th width="5%">Qty</th>
            <th width="15%">Price</th>
            <th width="10%" class="text-center">Action</th>
        </tr>
        @Html.EditorFor(m => m.UsedParts)

    </table>

    <div class="text-muted h6">
        <div class="discontinued" style="width:10px;height:10px; display:inline-block"></div>&nbsp;&nbsp;
        <em>Out of stock</em>
    </div>
</div>
